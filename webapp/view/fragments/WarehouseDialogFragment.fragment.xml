<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="No Warehouse Found"
		title="Select Warehouse"
		search="handleSearchWhs"
		confirm="handleValueHelpCloseWhs"
		cancel="handleValueHelpCloseWhs"
		showClearButton="true"
		items="{/AllWarehouses}">
		<StandardListItem
			iconDensityAware="false"
			iconInset="false"
			title="{WhsName}"
			description="Code : {WhsCode}" />
	</SelectDialog>
</core:FragmentDefinition>